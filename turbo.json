{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev:setup"]
    },
    "dev:setup": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "lint": {},
    "msw:init": {
      "outputs": [
        "public/mockServiceWorker.js"
      ]
    }
  }
}